# Configuración de la Base de Datos
MYSQL_DATABASE=nextclouddb
MYSQL_USER=nextclouduser
MYSQL_PASSWORD=cambia_este_password
MYSQL_ROOT_PASSWORD=cambia_este_root_password

# Configuración de GPG para cifrado
GPG_RECIPIENT=tu_email@ejemplo.com

# Configuración de AWS S3 (solo el nombre del bucket, sin s3://)
S3_BUCKET=nombre_de_tu_bucket

# Host de la base de datos (no cambiar si usas Docker Compose)
DB_HOST=db
